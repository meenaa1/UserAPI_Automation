								"exec": [
									"pm.test(\"Status code is 201\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test(\"Response has user_id\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('userId');",
									"    pm.collectionVariables.set(\"userId\", jsonData.userId);",
									"    ",
									"});",
									"",
									"pm.test(\"Response has addressId\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.userAddress).to.have.property('addressId');",
									"});",
									"",
									"pm.test(\"Response contains creation_time and last_mod_time\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('creationTime');",
									"    pm.expect(jsonData).to.have.property('lastModTime');",
									"});",
									"",
									"pm.test(\"User details match request\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.userFirstName).to.eql(\"John\");",
									"    pm.expect(jsonData.userLastName).to.eql(\"Doe\");",
									"});"
